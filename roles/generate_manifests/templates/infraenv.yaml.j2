apiVersion: agent-install.openshift.io/v1beta1
kind: InfraEnv
metadata:
  name: {{ cluster_name }}-infraenv
  namespace: cluster0
spec:
  clusterRef:
    name: {{ cluster_name }}
    namespace: cluster0
  pullSecretRef:
    name: pull-secret
  sshAuthorizedKey: {{ ssh_public_key }}
  nmStateConfigLabelSelector:
    matchLabels:
      cluster-name: {{ cluster_name }}
